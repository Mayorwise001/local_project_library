<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('header.ejs') %>
</head>
<body>
    <%- include('layout.ejs') %>
    <div class="col-sm-10">
        <h1><%= title %></h1>

<% if (author_list.length) { %>
  <ul>
    <% author_list.forEach(function(author) { %>
      <li>
        <a href="<%= author.url %>"><%= author.name %></a>
        <%= author.lifespan %> - (<%= author.lifespan2 %>)
      </li>
    <% }) %>
  </ul>
<% } else { %>
  <p>There are no authors.</p>
<% } %>

    </div>
</body>
</html>
